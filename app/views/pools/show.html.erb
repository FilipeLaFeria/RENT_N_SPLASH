<div class="container">

<div class="row">
  <div class="my_card d-flex justify-content-between col-12">

    <div class="card-img-info col-6">

      <img src="data:image/jpeg;base64,">
          <h3><%= @pool.title %></h3>
          <p><%= @pool.address %></p>
          <p><%= @pool.size %></p>
          <p><%= @pool.description %></p>
          <%= link_to "Back to list", pools_path, class: "btn btn-primary" %>
    </div>

    <div class="card-map-calendar col-6">
      <img src="https://joomly.net/frontend/web/images/googlemap/map.png">
      <div class="form-wrapper">Date
        <%= simple_form_for :search, method: "get" do |f| %>
          <div class="calendar-form d-flex">
            <%= f.input :date, as: :string, required: false, label: false, input_html: {class: "datepicker"}, style: "width: 100%" %>
            <%= f.button :submit, "Book this pool", class: "btn btn-primary form-control", style: "width: 50%"%></div>
        <% end %>
      </div>
    </div>

</div>
  </div>

</div>

